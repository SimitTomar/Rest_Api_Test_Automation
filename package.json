{
  "name": "rest_api_test_automation",
  "version": "1.0.0",
  "description": "Nodejs based Test Automation Framework for Testing of Rest APIs",
  "main": "index.js",
  "scripts": {
    "mocha_test": "mocha tests_mocha --reporter mochawesome",
    "test": "./node_modules/.bin/cucumber-js tests_cucumber/features/**/salary.feature -f json:tests_cucumber/reports/tmpJson/report.json && node report.js",
    "get_report": "node report.js",
    "cucumber_test": "node tests_cucumber/config.js --CUCUMBER_COMMAND=tests_cucumber/features/**/*.feature && node report.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SimitTomar/Rest_Api_Test_Automation.git"
  },
  "author": "Simit Tomar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SimitTomar/Rest_Api_Test_Automation/issues"
  },
  "homepage": "https://github.com/SimitTomar/Rest_Api_Test_Automation#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "fs-extra": "^9.0.0",
    "json-server": "^0.16.1",
    "mocha": "^7.0.1",
    "mochawesome": "^4.1.0",
    "nock": "^12.0.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "joi": "^14.3.1",
    "mocha-param": "^2.0.0",
    "util": "^0.12.1"
  }
}
